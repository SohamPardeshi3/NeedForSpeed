<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewEquipments">

    <Button
        android:id="@+id/AddtoCartbutton7"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="236dp"
        android:layout_height="41dp"
        android:layout_marginTop="48dp"
        android:onClick="addToCart"
        android:text="Add To Cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EnterQunatityeditTextNumber" />

    <Button
        android:id="@+id/Donebutton9"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="236dp"
        android:layout_height="41dp"
        android:layout_marginBottom="35dp"
        android:layout_marginTop="30dp"
        android:onClick="DoneRef"
        android:text="Done"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddtoCartbutton7"
        app:layout_constraintVertical_bias="0.133" />

    <EditText
        android:id="@+id/EnterQunatityeditTextNumber"
        android:layout_width="270dp"
        android:layout_height="47dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/recedittext"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EnterQuantitytextView10" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="304dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nfs" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="65dp"
        android:layout_height="33dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="24dp"
        android:text="From :"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/fromEquipTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/fromEquipTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="147dp"
        android:layout_marginTop="24dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="79dp"
        android:layout_height="29dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="24dp"
        android:text="Item:"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/quantityEquipTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="140dp"
        android:layout_marginTop="24dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fromEquipTextView" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="119dp"
        android:layout_height="58dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="24dp"
        android:text="Item Rate (Per day):"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/rateEquipTextView"
        android:layout_width="158dp"
        android:layout_height="55dp"
        android:layout_marginEnd="91dp"
        android:layout_marginTop="24dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/quantityEquipTextView" />

    <TextView
        android:id="@+id/EnterQuantitytextView10"
        android:layout_width="318dp"
        android:layout_height="58dp"
        android:layout_marginTop="39dp"
        android:fontFamily="@font/allerta"
        android:text="How many Days you want to rent the truck?"
        android:textAlignment="center"
        android:textColor="#4CAF50"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

</androidx.constraintlayout.widget.ConstraintLayout>