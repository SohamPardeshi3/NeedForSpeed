<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewItems">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="304dp"
        android:layout_height="55dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nfs" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="65dp"
        android:layout_height="33dp"
        android:layout_marginStart="33dp"
        android:layout_marginTop="24dp"
        android:text="From :"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/fromItemsTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/fromItemsTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="147dp"
        android:layout_marginTop="24dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="125dp"
        android:layout_height="33dp"
        android:layout_marginEnd="17dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:text="Item Name :"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/typeItemsTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/typeItemsTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="77dp"
        android:layout_marginTop="21dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/fromItemsTextView" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="145dp"
        android:layout_height="33dp"
        android:layout_marginEnd="7dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="24dp"
        android:text="Item Quantity :"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/quantityItemsTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/quantityItemsTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="77dp"
        android:layout_marginTop="24dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/typeItemsTextView" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="125dp"
        android:layout_height="33dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="24dp"
        android:text="Item Rate :"
        android:textColor="#4CAF50"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/rateItemsTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/rateItemsTextView"
        android:layout_width="151dp"
        android:layout_height="33dp"
        android:layout_marginEnd="111dp"
        android:layout_marginTop="25dp"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/quantityItemsTextView" />

</androidx.constraintlayout.widget.ConstraintLayout>