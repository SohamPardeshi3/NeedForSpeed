<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Received_Orders_Wholesaler">

    <ImageButton
        android:id="@+id/seedOrders"
        android:layout_width="330dp"
        android:layout_height="150dp"
        android:layout_marginTop="32dp"
        android:background="#FAFAFA"
        android:onClick="seedOrders"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/fertOrders"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/sellseeds" />

    <ImageButton
        android:id="@+id/fertOrders"
        android:layout_width="330dp"
        android:layout_height="150dp"
        android:layout_marginTop="32dp"
        android:background="#FAFAFA"
        android:onClick="fertOrders"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/equipOrders"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seedOrders"
        app:srcCompat="@drawable/sellfert" />

    <ImageButton
        android:id="@+id/equipOrders"
        android:layout_width="350dp"
        android:layout_height="150dp"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="32dp"
        android:background="#FAFAFA"
        android:onClick="equipOrders"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fertOrders"
        app:srcCompat="@drawable/sellequip" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="240dp"
        android:layout_height="40dp"
        android:layout_marginEnd="155dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="16dp"
        android:text="Select Category :"
        android:textColor="#4CAF50"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_marginEnd="71dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="20dp"
        android:contentDescription="TODO"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/nfs" />

</androidx.constraintlayout.widget.ConstraintLayout>